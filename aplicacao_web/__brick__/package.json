{
	"name": "{{name.lowerCase().paramCase()}}",
	"description": "{{description}}",
	"author": "{{author}}",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint . --fix",
		"format": "prettier --plugin-search-dir . --write .",
		{{#useGitHubPagesAdapter}}
		"deploy": "npx gh-pages -d build -b gh-pages -r https://github.com/{{author}}/{{name.lowerCase().paramCase()}} -t true --no-jekyll",
		{{/useGitHubPagesAdapter}}
		"typesafe-i18n": "typesafe-i18n"
	},
	"devDependencies": {
		{{#useNodeAdapter}}
		"@sveltejs/adapter-node": "^1.2.1",
		{{/useNodeAdapter}}
		{{#useGitHubPagesAdapter}}
		"@sveltejs/adapter-static": "^2.0.1",
		{{/useGitHubPagesAdapter}}
		"@sveltejs/kit": "^1.20.4",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte":"^2.32.2",
		"eslint-plugin-unused-imports":"^2.0.0",
		"postcss": "^8.4.21",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.2",
		"prettier-plugin-tailwindcss": "^0.5.13",
		"svelte": "^4.0.0",
		"svelte-check": "^3.4.3",
		"tailwindcss": "^3.2.7",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.3.9"
	},
	"type": "module",
	"dependencies": {
		"rippleui": "^1.9.2",
		"typesafe-i18n": "^5.24.2"
	},
	"keywords": [
		{{#keywords}}
			"{{.}}",
		{{/keywords}}
		"{{name.lowerCase().paramCase()}}"
	]
}